# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Hugging Face API Key (for AI models)
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Google OAuth Credentials
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Google Maps API Key
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Firebase Credentials (Base64 encoded JSON)
FIREBASE_CREDENTIALS_JSON=your_base64_encoded_firebase_credentials_here

# SERP API Key (for search functionality)
SERP_API_KEY=your_serp_api_key_here

# Google Generative AI API Key
GENAI_API_KEY=your_genai_api_key_here

# Flask Configuration
FLASK_SECRET_KEY=your-very-secret-dev-key-here
FLASK_ENV=development
FLASK_DEBUG=True
